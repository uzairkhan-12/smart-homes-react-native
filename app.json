{
  "expo": {
    "name": "PrimeWave",
    "slug": "primewave",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/primewave.png",
    "scheme": "primewave",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.uzairkhan12.primewave",
      "supportsTablet": true,
      "requireFullScreen": false
    },
    "android": {
      "package": "com.uzairkhan12.primewave",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/primewave.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "usesCleartextTraffic": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/primewave.png"
    },
    "plugins": [
      "expo-router",
      "./plugins/with-network-security-config.js",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/primewave.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "88b5932d-1077-44bd-bc22-6ea1d01b318d"
      }
    }
  }
}
